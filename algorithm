 def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        longest_len = 0
        current_len = 0
        cursub=[]
        s_list = list(s)
        for i in range(len(s_list)):
            if not cursub.count(s_list[i]):
                cursub.append(s_list[i])
                
            else:
                current_len = len(cursub)
                if current_len > longest_len:
                    longest_len = current_len
                if cursub.pop() == s_list[i]:
                    cursub = [s_list[i]]
                else:
                    cursub.append(s_list[i-1])
                index = cursub.index(s_list[i])
                repeat_ = cursub
                for j in range(index+1):
                    cursub.remove(repeat_[j])
                cursub.append(s_list[i])
            if i == len(s_list)-1:
                current_len = len(cursub)
                if current_len > longest_len:
                    longest_len = current_len
                
        return longest_len
