class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        longest_len = 0
        current_len = 0
        cursub=[]
        del_index = 0
        for i in range(len(s)):
            if not s[i] in cursub:
                cursub.append(s[i])
                
            else:
                current_len = len(cursub)
                if current_len > longest_len:
                    longest_len = current_len
                
                while s[i] in cursub:
                    del_index += 1
                    cursub.remove(s[del_index])
                    
                cursub.append(s[i])
            if i == len(s)-1:
                current_len = len(cursub)
                if current_len > longest_len:
                    longest_len = current_len
                
        return longest_len
